{
  "main": "dist/main.js",
  "dependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/pdfkit": "^0.12.6",
    "axios": "^0.27.2",
    "ci": "^2.2.0",
    "cli-progress": "^3.11.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.25",
    "octokit": "^1.7.1",
    "pdfkit": "^0.13.0",
    "typescript": "^4.6.4",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "chai": "^4.3.6",
    "eslint": "^8.15.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "ts-node": "^10.8.0"
  },
  "optionalDependencies": {
    "fsevents": "2.1.2"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  },
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts'",
    "test_windows": "mocha -r ts-node/register 'test/**/*.ts'",
    "test:coverage": "nyc npm run test",
    "start": "tsc && node build/main.js"
  }
}
