{
  "main": "dist/main.js",
  "type": "module",
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^5.1.0",
    "ci": "^2.2.0",
    "cli-progress": "^3.11.1",
    "dotenv": "^16.0.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.25",
    "mocha": "^10.0.0",
    "octokit": "^1.8.1",
    "typescript": "^4.6.4",
    "winston": "^3.7.2",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@octokit/types": "^7.1.0",
    "@types/chai": "^4.3.1",
    "@types/cli-progress": "^3.11.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^17.0.35",
    "@types/sinon": "^10.0.13",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "chai": "^4.3.6",
    "eslint": "8.22.0",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.8.0"
  },
  "optionalDependencies": {
    "fsevents": "2.1.2"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  },
  "scripts": {
    "test": "ts-mocha -n es-module-specifier-resolution=node -n loader=ts-node/esm/transpile-only -p tsconfig.json 'src/**/*.test.ts'",
    "test:coverage": "nyc npm run test",
    "start": "node --no-warnings --es-module-specifier-resolution=node --loader ts-node/esm/transpile-only src/Main.ts",
    "license_checker": "node --no-warnings --es-module-specifier-resolution=node --loader ts-node/esm/transpile-only src/Main.ts"
  }
}
